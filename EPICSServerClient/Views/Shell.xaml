<UserControl x:Class="EPICSServerClient.Views.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EPICSServerClient.Views"
        xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:helpers="clr-namespace:EPICSServerClient.Helpers.Constants;assembly=ScheibApps.EPICSServerClient.Helpers"
        x:Name="ShellWindow"
        mc:Ignorable="d" prism:ViewModelLocator.AutoWireViewModel="True" >

    <ContentControl Grid.Row="0" Name="ShellView" prism:RegionManager.RegionName="{x:Static helpers:RegionConstants.ShellRegion}"></ContentControl>
    
</UserControl>

